{
  "name": "i18n-for-browser",
  "version": "1.0.1",
  "description": "Implementation of i18n-node designed for client-side.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/i18n-for-browser"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/i18n-for-browser/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "umd": "lib/umd.js",
  "scripts": {
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "checkSize": "size-limit",
    "test": "npm run lint && npm run jest && npm run build && npm run checkSize",
    "build:docs": "typedoc ./src --out ./docs --ignoreCompilerErrors --excludeExternals --mode modules; touch docs/.nojekyll",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "cleanPublish": "npm run test && clean-publish"
  },
  "keywords": [
    "template",
    "i18n",
    "l10n",
    "browser",
    "client",
    "client-side"
  ],
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@types/js-cookie": "^2.2.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10.12.6",
    "@types/sprintf-js": "^1.1.1",
    "assert": "^1.4.1",
    "js-cookie": "^2.2.0",
    "make-plural": "^4.0.1",
    "math-interval-parser": "^2.0.0",
    "messageformat": "^2.0.4",
    "mustache": "^3.0.1",
    "sprintf-js": ">=1.0.3",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@types/jest": "^23.3.9",
    "babel-preset-trigen": "^1.7.1",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "rollup": "^0.67.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-tslint": "^0.1.36",
    "rollup-plugin-typescript2": "^0.18.0",
    "size-limit": "^0.21.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-trigen": "^3.0.6",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "files": [
    "lib"
  ]
}
